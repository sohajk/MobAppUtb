<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mobapputb.ui.notes.NotesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_notes" />

    <fragment
        android:id="@+id/navigation_weather"
        android:name="com.example.mobapputb.ui.weather.WeatherFragment"
        android:label="@string/title_weather"
        tools:layout="@layout/fragment_weather" />
    <fragment
        android:id="@+id/navigation_note_add"
        android:name="com.example.mobapputb.ui.notes.NoteAddFragment"
        android:label="fragment_note_add"
        tools:layout="@layout/fragment_note_add" />

</navigation>